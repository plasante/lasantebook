<% if @items == 'books' %>
	<%= render :partial => 'book/list_books', :locals => {:books => @books} %>
<% elsif @items == 'dvds' %>
    <%= render :partial => 'dvd/list_dvds', :locals => {:dvds => @dvds} %>
<% else %>
    <%= render :partial => 'cd/list_cds', :locals => {:cds => @cds}%>
<% end %>
